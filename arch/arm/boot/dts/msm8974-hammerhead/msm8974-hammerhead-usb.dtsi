&usb3 {
	compatible = "qcom,dwc-usb3-msm";
	reg = <0xf9200000 0xfc000>,
		<0xfd4ab000 0x4>;
	qcom,dwc-usb3-msm-adc-low-threshold = <100>;
	qcom,dwc-usb3-msm-adc-high-threshold = <110>;
	qcom,dwc-hsphy-init = <0x00D1DDA4>;
	qcom,dwc-hsphy-host-init = <0x00D191A4>;

	dwc3@f9200000 {
		no-set-vbus-power;
	};
};
